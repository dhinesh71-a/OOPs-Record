Test case 1
package exceptionhandling;

public class Project7_ExceptionHandling {
    public static void main(String[] args) {
        System.out.println("Name = DHINESH KUMAR G");
        System.out.println("Exam Registration Number = 2117240070071");

        int a = 10, b = 2;
        try {
            int result = a / b;
            System.out.println("Result = " + result);
        } catch (ArithmeticException e) {
            System.out.println("Exception handled: Division by zero");
        }
    }
}

