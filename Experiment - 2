        int num = sc.nextInt();
        int count = 0;

        if (num <= 1) {
            System.out.println("Not a prime number");
        } else {
            for (int i = 1; i <= num; i++) {
                if (num % i == 0)
                    count++;
            }
            if (count == 2)
                System.out.println("Prime number");
            else
                System.out.println("Not a prime number");
        }
    }
